{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "prisma", 
      "use": "@prisma/cli"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],

  "functions": {
    "prisma-generate": {
      "handler": "prisma/generate.handler",
      "includeFiles": "/prisma/**" 
    }
  }
}
