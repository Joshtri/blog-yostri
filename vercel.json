{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "buildCommand": "next build"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],

  "functions": {
    "prisma-generate": {

      "includeFiles": "prisma/**" 
    }
  }
}
